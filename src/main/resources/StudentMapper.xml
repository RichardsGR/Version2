<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sasc.mapper.StudentMapper">
    <insert id="add" parameterType="com.sasc.pojo.Student"  useGeneratedKeys="true" keyProperty="id">
        INSERT INTO tb_student (name,num,phone,qq)VALUES(#{name},#{num},#{phone},#{qq})
    </insert>
</mapper>

